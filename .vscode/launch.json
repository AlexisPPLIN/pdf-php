{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Listen for XDebug",
            "type": "php",
            "request": "launch",
            "port": 9000,
            "localSourceRoot": "${workspaceRoot}"
        },
        {
            "name": "Launch currently open script",
            "type": "php",
            "request": "launch",
            "program": "${file}",
            "cwd": "${fileDirname}",
            "port": 9000
        },
        {
            "name": "Launch as Serer",
            "type": "powershell",
            "request": "launch",
            "program": "F:/Entwicklung/php-5.6.19/php.exe",
            "args": ["-S localhost:80"],
            "cwd": "${workspaceRoot}",
            "port": 9000
        }
    ]
}